{
  "Features": {
    "EnableSignalR": false,
    "EnableTracing": false,
    "SendToInflux": false,
    "SendToKnxSender": false,
    "SendToLog": false,
    "SendToSignalR": false
  },

  "Cors": [
    "http://localhost:9000"
  ],

  "KnxSender": {
    "Endpoint": ""
  },

  "InfluxDb": {
    "Endpoint": "",
    "Token": "",
    "Bucket": "",
    "Organisation": "",
    "LogLevel": "None"
  },

  "Tracing": {
    "Host": "",
    "Port": 0,
    "ServiceName": "FunctionalLiving.Api"
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Error",
        "Microsoft.AspNetCore.Mvc.Infrastructure": "Error",
        "Be.Vlaanderen.Basisregisters.AspNetCore.Mvc.Logging": "Information",
        "FunctionalLiving.Knx.KnxCommandHandlerModule": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} <s:{SourceContext}>{NewLine}{Exception}"
        }
      }
    ],
    "Properties": {
      "Application": "FunctionalLiving - API"
    }
  }
}
